	journaltitle = {Endocrine Reviews},
	langid = {english},
	number = {6},
	pages = {584--608},
	shorttitle = {Circadian {{Rhythm}} and {{Sleep Disruption}}},
	title = {Circadian {{Rhythm}} and {{Sleep Disruption}}: {{Causes}}, {{Metabolic Consequences}}, and {{Countermeasures}}},
