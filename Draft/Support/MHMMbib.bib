	eprinttype = {pmid},
	file = {/Users/aronjr/Zotero/storage/4PBR73MK/John et al. - 2019 - An Open-Source Monitor-Independent Movement Summar.pdf},
	issn = {2575-6613},
	journaltitle = {Journal for the Measurement of Physical Behaviour},
	keywords = {activity count,activity monitor,MIMS,NHANES,physical activity measurement},
	langid = {english},
	number = {4},
	pages = {268--281},
	pmcid = {PMC8301210},
	shortjournal = {J Meas Phys Behav},
	title = {An {{Open-Source Monitor-Independent Movement Summary}} for {{Accelerometer Data Processing}}},
