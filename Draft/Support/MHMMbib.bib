	issn = {1424-8220},
	journaltitle = {Sensors (Basel, Switzerland)},
	keywords = {Electroencephalography,Humans,Photoplethysmography,Polysomnography,sleep,Sleep,sleep scoring,sleep sensors,Sleep Stages,sleep staging,Wearable Electronic Devices,wearables},
	langid = {english},
